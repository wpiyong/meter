<Window x:Class="gUV.View.SpectrumLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Spectrum login required " Height="200" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10" FocusManager.FocusedElement="{Binding ElementName=spectrumUser}">
        <Label>Username:</Label>
        <TextBox x:Name="spectrumUser"/>
        <Label>Password:</Label>
        <PasswordBox x:Name="spectrumDat" />
        <StackPanel Orientation="Horizontal" Margin="0,20,0,0" HorizontalAlignment="Right">
            <Button x:Name="OK" Width="70" Margin="0,0,5,0" Click="OK_Click">OK</Button>
            <Button x:Name="Cancel" Width="70" Click="Cancel_Click">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
